<div class="section__content--button-wrapper nav__item--button-wrapper"{% if section['onenter'] != false %} data-interaction="{{section['onenter']}}"{% endif %}>
    {% for value, item in btnRef.btn %}
    <div class="section__content--button section__content--button-{{ item.btnSize }}">
        <a class="nav__item nav__item--button-{{ item.btnSize }} nav__item--button-{{ item.btnType }}" href="{{ item.btnHref }}">{{ item.btnText | safe }}</a>
    </div>
    {% endfor %}
</div>